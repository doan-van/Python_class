








# python 配列
import numpy as np
x = np.arange(-10,10,1)
y = x**2 + 2.*x  + 10

print('x', x)
print('y', y)





import matplotlib.pyplot as plt


plt.plot(x,y)
plt.show()





x = np.arange(-10,10,0.1)
y = np.cos(x)
plt.plot(x,y)
plt.show()


plt.plot(x,y, ls='--',marker='o', color='g')
plt.xlabel('x label', fontsize=20)
plt.ylabel('unit', fontsize=12, color='r')





